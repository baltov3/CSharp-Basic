int n = int.Parse(Console.ReadLine());
int current = 1;
bool isBigger = false;
for (int i = 0; i < n; i++)
{
	for (int j = 0; j <= i; j++)
	{
		if (current>n)
		{
            isBigger=true;
			break;

        }
		Console.Write(current + " ");
		current++;
	}
	if (isBigger)
	{
		break;
	}
	Console.WriteLine();
}
