using System;

class Program
{
    static void Main()
    {
       
        int n = int.Parse(Console.ReadLine());

        if (n > 1)
        {
            for (int i = n; i >= 1; i--)
            {
                Console.WriteLine(i);
            }
        }
        else
        {
            Console.WriteLine("1");
        }
    }
}
