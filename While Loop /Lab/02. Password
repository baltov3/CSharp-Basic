using System;

class Program
{
    static void Main()
    {
        // Четем името и първоначалната парола на потребителския профил
        string username = Console.ReadLine();
        string initialPassword = Console.ReadLine();

        // Четем парола за вход от потребителя
        string inputPassword = Console.ReadLine();

        // Проверка за правилност на въведената парола
        while (inputPassword != initialPassword)
        {
           
            inputPassword = Console.ReadLine();
        }

        // Извеждаме съобщение за успешен вход
        Console.WriteLine($"Welcome {username}!");
    }
}
