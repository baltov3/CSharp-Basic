class Program
{
    static void Main()
    {
        // Въвеждане на цяло число от потребителя
       
        
        // Обработка на входа и проверка за грешки
        if (int.TryParse(Console.ReadLine(), out int userInput))
        {
            // Проверка на стойността на въведеното число
            if (userInput < 100)
            {
                Console.WriteLine("Less than 100");
            }
            else if (userInput >= 100 && userInput <= 200)
            {
                Console.WriteLine("Between 100 and 200");
            }
            else
            {
                Console.WriteLine("Greater than 200");
            }
        }
       
        // Изчакване на потребителски вход преди затваряне на конзолата
        Console.ReadLine();
    }
}
